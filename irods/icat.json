{
    "key": "icat",
    "label": "iRODS iCAT",
    "requiresVolume": true,
	"description": "open source data management software",
    "image": "ndslabs/irods-icat:NDS-292",
	"access": "external",
	"display": "stack",
    "ports": [
        {
            "port": 1247,
            "protocol": "tcp"
        },
        {
            "port": 8080,
            "protocol": "http"
        }
    ],
    "config":  [
        {
            "name": "RODS_ZONE",
            "value": "fedZone",
            "label": "iRODS zone",
            "canOverride": true
        },
        {
            "name": "RODS_PASSWORD",
            "value": "",
            "label": "iRODS rods user password",
            "canOverride": true,
            "isPassword": true
        }
    ],
	"volumeMounts":[
	   { "mountPath": "/var/lib/irods/iRODS/Vault", "name": "icat" }
    ],
    "depends": [
      { "key": "cloudbrowserui", "required": false }
    ]
}

{
    "key": "cloudbrowser",
    "label": "iRODS Cloudbrowser API",
    "requiresVolume": false,
    "image": "ndslabs/cloudbrowser:latest",
    "access": "external",
    "ports": [
        {
            "port": 8009,
            "protocol": "tcp"
        }
    ],
    "config":  [
        {
            "name": "IRODS_ZONE",
            "value": "",
            "label": "iRODS Zone",
            "canOverride": true
        }
	],
    "resourceLimits": {
        "cpuMax": "500m",
        "cpuDefault": "100m",
        "memMax": "2Gi",
        "memDefault": "500Mi"
    }
}

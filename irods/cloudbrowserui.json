{
    "key": "cloudbrowserui",
    "label": "iRODS Cloudbrowser UI",
    "requiresVolume": false,
    "image": "ndslabs/cloudbrowser-ui:latest",
	"access": "external",
	"display": "standalone",
    "ports": [
        {
            "port": 80,
            "protocol": "http"
        }
    ],
    "depends": [
      { "key": "cloudbrowser", "required": true }
    ],
    "readinessProbe" : {
        "type" : "http",
        "path" : "app/images/beta_test_logo.png",
        "port" : 80,
        "initialDelay": 10,
        "timeout" : 600
    },
    "resourceLimits": {
        "cpuMax": "250m",
        "cpuDefault": "100m",
        "memMax": "250Mi",
        "memDefault": "100Mi"
    }
}

{
    "key": "toolserver",
    "label": "Tool Server",
    "description": "Docker tool server",
    "logo": "https://nationaldataservice.atlassian.net/wiki/download/attachments/13369353/ndsabs-small.png",
    "info": "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Tool+Server",
    "maintainer": "",
    "image": {
        "registry": "",
        "name": "craigwillis/toolserver",
        "tags": [
            "latest"
        ]
    },
    "display": "stack",
    "access": "internal",
    "ports": [
        {
            "port": 8082,
            "protocol": "http"
        }
    ],
    "repositories": null,
    "readinessProbe": {
        "type": "tcp",
        "path": "/tools",
        "port": 8082,
        "initialDelay": 30,
        "timeout": 120
    },
    "resourceLimits": {
        "cpuMax": 1000,
        "cpuDefault": 100,
        "memMax": 2000,
        "memDefault": 1000
    },
    "privileged": true
}

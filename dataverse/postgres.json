{
    "key": "postgres",
	"access": "internal",
	"display": "standalone",
    "label": "PostgreSQL",
    "image": "postgres:9.3",
    "ports": [
        { "port": 5432, "protocol": "tcp" }
    ],
    "volumeMounts":[
        { "mountPath": "/var/lib/postgresql/data", "name": "postgres" }
    ],
    "resourceLimits": {
        "cpuMax": "1",
        "cpuDefault": "100m",
        "memMax": "250Mi",
        "memDefault": "100Mi"
    }
}

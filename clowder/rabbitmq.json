{
    "key": "rabbitmq",
    "label": "RabbitMQ",
    "description": "A highly reliable enterprise messaging system",
    "image": "rabbitmq:3.6.1-management",
	"display": "standalone",
	"access": "internal",
    "ports": [
        { "port": 5672, "protocol": "tcp" },
        { "port": 15672, "protocol": "tcp" }
    ],
    "readinessProbe" : {
        "type" : "tcp",
        "path" : "unused",
        "port" : 5672,
        "initialDelay": 30
    },
    "resourceLimits": {
        "cpuMax": "1",
        "cpuDefault": "100m",
        "memMax": "500Mi",
        "memDefault": "150Mi"
    }
}

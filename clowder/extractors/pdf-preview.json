{
    "key": "pdfpreview",
    "label": "PDF Preview Extractor",
    "description": "Clowder extractor for PDF preview thumbnails",
    "image": "ndslabs/pdf-preview:0.9.2",
    "access": "internal",
    "depends": [
        {
            "key": "rabbitmq",
            "required": true
        }
    ],
    "config":  [
        {
            "name": "RABBITMQ_EXCHANGE",
            "value": "clowder",
            "canOverride": false
        },
        {
            "name": "RABBITMQ_VHOST",
            "value": "%2F",
            "canOverride": false
        },
        {
            "name": "RABBITMQ_QUEUE",
            "value": "ncsa.pdf.preview",
            "canOverride": false
        }
    ]
}

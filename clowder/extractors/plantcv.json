{
    "key": "plantcv",
    "label": "PlantCV extractor",
    "description": "Clowder extractor for PlantCV images",
    "image": "ndslabs/plantcv:0.9.2",
	"access": "internal",
    "depends": [
        {
            "key": "rabbitmq",
            "required": true
        }
    ],
    "config":  [
        {
            "name": "RABBITMQ_EXCHANGE",
            "value": "clowder",
            "canOverride": false
        },
        {
            "name": "RABBITMQ_VHOST",
            "value": "%2F",
            "canOverride": false
        },
        {
            "name": "RABBITMQ_QUEUE",
            "value": "terra.plantcv",
            "canOverride": false
        }
    ],
    "resourceLimits": {
        "cpuMax": "1",
        "cpuDefault": "100m",
        "memMax": "250Mi",
        "memDefault": "100Mi"
    }
}

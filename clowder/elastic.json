{
    "key": "elasticsearch",
    "label": "ElasticSearch 1.x",
    "description": "A distributed search and analytics engine",
    "image": "elasticsearch:1.3",
	"access": "internal",
    "ports": [
        { "port": 9300, "protocol": "tcp" }
    ],
    "args" : [
        "-Des.cluster.name=clowder" 
    ],
    "resourceLimits": {
        "cpuMax": "1",
        "cpuDefault": "100m",
        "memMax": "500Mi",
        "memDefault": "250Mi"
    }
}
